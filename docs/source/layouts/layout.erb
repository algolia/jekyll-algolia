<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <link href='#' rel='icon' type='image/x-icon'>
    <meta content='#' name='description'>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport'>
    <!-- / Twitter card -->
    <meta content='summary_large_image' name='twitter:card'>
    <meta content='@Algolia' name='twitter:site'>
    <meta content='@Algolia' name='twitter:creator'>
    <meta content="Algolia Product" name="twitter:title">
    <meta content="Algolia Product description" name="twitter:description">
    <meta content="Algolia Product image" name="twitter:image">
    <!-- / OG meta -->
    <meta content='#' property='og:url'>
    <meta content='#' property='og:title'>
    <meta content='#' property='og:image'>
    <meta content='article' property='og:type'>
    <meta content='#' property='og:description'>
    <meta content='IMDb' property='og:site_name'>


    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Middleman" %></title>

    <link href='https://fonts.googleapis.com/css?family=Raleway:400,500,600' rel='stylesheet' type='text/css'>

    <%= stylesheet_link_tag :site %>

  </head>

  <body class="<%= page_classes %>">
    <%= partial('partials/header') =%>
    <%= yield %>
    <%= partial('partials/footer') =%>
  </body>
  <script src="javascripts/all.js"></script>
  <script src="javascripts/responsiveNavigation.js"></script>
</html>
